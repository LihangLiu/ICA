function plot_signals( X,Y )
% X: (n, t), original signal
% Y: (n,t), recovered signal
% scale to [0,1]

    [n,t] = size(X);
    % normlize 
    X = norm_signals(X);
    Y = norm_signals(Y);
    % align
    [X,Y] = align_signals(X,Y);
    
    % plot
    figure;
    colors = ['b','g','r','c','m','y','k'];
    for i=1:n
        subplot(n,2,i*2-1);
        plot(X(i,:), colors(i));
        subplot(n,2,i*2);
        plot(Y(i,:),colors(i));
    end
   

end

